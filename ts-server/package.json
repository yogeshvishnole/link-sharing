    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.6",
    "nodemon": "^2.0.6",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/express": "^4.17.10",
    "@types/mongoose": "^5.10.3",
    "aws-sdk": "^2.825.0",
    "cors": "^2.8.5",
